<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1A0C38"
    tools:context=".ui.wrappedui" >

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/mainwrappedbg" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="73dp"
        android:layout_height="54dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/app_name"
        android:src="@android:drawable/ic_menu_send"
        android:translationX="175dp"
        android:translationY="700dp" />

    <ListView
        android:id="@+id/songWrappedList"
        android:layout_width="245dp"
        android:layout_height="268dp"
        android:background="@android:color/transparent"
        android:cacheColorHint="@android:color/transparent"
        android:translationX="90dp"
        android:translationY="140dp" />

    <ListView
        android:id="@+id/artistWrappedList"
        android:layout_width="245dp"
        android:layout_height="239dp"
        android:background="@android:color/transparent"
        android:cacheColorHint="@android:color/transparent"
        android:translationX="90dp"
        android:translationY="440dp" />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/wrappedClickRight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:rotation="0"
        android:src="@android:drawable/ic_media_play"
        android:translationX="352dp"
        android:translationY="380dp"
        app:backgroundTint="#8D4E8CD5" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/wrappedClickLeft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:rotation="180"
        android:src="@android:drawable/ic_media_play"
        android:translationX="0dp"
        android:translationY="380dp"
        app:backgroundTint="#8D4E8CD5" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Top Songs"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold"
        android:translationX="90dp"
        android:translationY="110dp"
        tools:text="Top Songs" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Top Artists"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold"
        android:translationX="90dp"
        android:translationY="400dp"
        tools:text="Top Artists" />


</FrameLayout>